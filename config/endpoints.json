{
  "provider": "jsonplaceholder",
  "baseUrl": "https://jsonplaceholder.typicode.com",
  "endpoints": {
    "posts": {
      "list": {
        "method": "GET",
        "path": "/posts",
        "expect": ["userId", "id", "title", "body"]
      },
      "create": {
        "method": "POST",
        "path": "/posts",
        "sampleBody": { "title": "foo", "body": "bar", "userId": 1 },
        "expect": ["id", "title", "body", "userId"]
      }
    }
  }
}